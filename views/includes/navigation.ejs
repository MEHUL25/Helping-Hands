<!-- Start header section -->
 <header id="aa-header">
  <!-- start header top  -->
  <div class="aa-header-top">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="aa-header-top-area">
            <div class="aa-header-top-right">
              <ul class="aa-head-top-nav-right">
                <% if (!isAuthenticated) { %>
                      <li><a href="/login">Register/Login</a></li>
                <% } else { %>  
                  <form action="/login/logout" method="post">
                      <button class="aa-browse-btn" type="submit">Logout</button>
                  </form>
                  <% } %>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- / header top  -->

  <!-- start header bottom  -->
  <div class="aa-header-bottom">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="aa-header-bottom-area">
            <!-- logo  -->
            <div class="aa-logo">
              <!-- Text based logo -->
              <a href="/">
                <span class="fa fa-logo"></span>
                <p><strong>Feeding</strong> <span>hands</span></p>
              </a>
              <!-- img based logo -->
              <!-- <a href="index.html"><img src="img/logo.jpg" alt="logo img"></a> -->
            </div>
            <!-- / logo  -->

            <% if (path=="/watchlist") { %>
              
                
              <!-- search box -->

              <div class="inline">

                  <form  action="/Watchlist" method="POST">

                    
                        <!-- cart box -->
                          <div class="aa-cartbox">
                            <div class="dropdown" >
                                <select id="choice" class="aa-browse-btn dropdown-toggle" aria-labelledby="dropdownMenuButton" name="choice" data-toggle="dropdown" aria-haspopup="true" >
                                      <option value="Latest" selected >Date</option>
                                      <option value="id" >ID</option>
                                      <option value="consumable">Consumable</option>
                                      <option value="nonconsumable">Non Consumable</option>
                                      <option value="city">City</option>
                                      <option value="available">Available</option>
                                </select>
                              </div>
                          </div>
                          <!-- / cart box -->

                          <div class="aa-search-box" >
                            <input type="text" name="search" id="search" placeholder="Search here ">
                            <button type="submit"><span class="fa fa-search"></span></button>
                                </div>
                          
                </form>

              </div>
              <!-- / search box -->
              
                <% } %>  





          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- / header bottom  -->
</header>
<!-- / header section -->
